; delete up to this portion 

; defines structure of user input 
(deftemplate UserInput
   (slot value)
   (slot genre (type STRING))
   (slot artist (type STRING))
   (slot beats_per_minute)
   (slot year)
   (slot popularity)
   (slot length)
   (slot mood)
) 

; program start with homepage menu 
(defrule get-user-input
   =>
   (printout t crlf crlf)
   (printout t "============ Music Recommendation System ============" crlf)
   (printout t "1. Recommend based on genre" crlf)
   (printout t "2. Recommend based on artist" crlf)
   (printout t "3. Recommend based on beats" crlf)
   (printout t "4. Recommend based on year" crlf)
   (printout t "5. Recommend based on popularity" crlf)
   (printout t "6. Recommend based on length" crlf)
   (printout t "7. Recommend based on mood" crlf)
   (printout t "8. Exit program" crlf)
   (printout t "======================================================" crlf)
   (printout t "Enter a Value from the Menu: ")
   (bind ?value (read))
   (assert (UserInput (value ?value)))
)

; case 1, recommend song by genre
(defrule recommend-by-genre
   (UserInput (value 1))
   =>
   (printout t "Enter the genre you like: ")
   (bind ?userGenre (readline))
   (bind ?userGenre (lowcase ?userGenre)) ; Convert input to lowercase for case-insensitive comparison
   (printout t crlf crlf)
   (assert (GenreSearchStarted))
   (printout t "Recommended songs in the genre " ?userGenre ":" crlf)
   (do-for-all-facts ((?m Music)) 
      (if (eq (str-compare (lowcase (nth$ 1 ?m:top_genre)) ?userGenre) 0) 
         then 
         (printout t "Title: " (nth$ 1 ?m:title) ", Artist: " (nth$ 1 ?m:artist) crlf)
      )
   )
   (assert (GenreSearchCompleted))
)

; Fallback rule when no songs are found in the specified genre
(defrule no-songs-found-in-genre
   (UserInput (value 1))
   (GenreSearchStarted)
   (not (GenreSearchCompleted))
   =>
   (printout t "No songs found for the specified genre." crlf)
)


; case 2, recommend song by artist
(defrule recommend-by-artist
   (UserInput (value 2))
   =>
   (printout t "Enter the name of an artist you like: ")
   (bind ?userArtist (readline)) ; Use readline to capture the entire user input as a string
   (bind ?userArtist (lowcase ?userArtist)) ; Convert input to lowercase for case-insensitive comparison
   (printout t crlf crlf)
   (assert (ArtistSearchStarted))
   (printout t "Recommended songs by " ?userArtist ":" crlf) 
   (do-for-all-facts ((?m Music)) 
      (if (eq (str-compare (lowcase (nth$ 1 ?m:artist)) ?userArtist) 0) 
         then 
         (printout t "Title: " (nth$ 1 ?m:title) ", Artist: " (nth$ 1 ?m:artist) crlf)
      )
   )
   (assert (ArtistSearchCompleted))
)

; Fallback rule when no songs are found for the specified artist
(defrule no-songs-found-for-artist
   (UserInput (value 2))
   (ArtistSearchStarted)
   (not (ArtistSearchCompleted))
   =>
   (printout t "No songs found for the specified artist." crlf)
)


; case 3, recommend song by beats_per_minute 
(defrule recommend-by-beats
   (UserInput (value 3))
   =>
   (printout t "Enter the beats-per-minute you prefer (between 70 - 200): ")
   (bind ?userBeats (read))
   (printout t crlf crlf)

   ; Check if there are any songs with the exact BPM
   (bind ?songsWithExactBPM (find-all-facts ((?m Music)) (= ?m:beats_per_minute ?userBeats)))

   ; If songs with the exact BPM exist, recommend them
   (if (> (length$ ?songsWithExactBPM) 0)
      then
      (printout t "Recommended songs with beats per minute of " ?userBeats ":" crlf)
      (do-for-fact ((?m ?songsWithExactBPM))
         (printout t "Title: " ?m:title ", Artist: " ?m:artist ", BPM: " ?m:beats_per_minute crlf)
      )
      else

      ; Define a range (e.g., +/- 5 beats per minute) if no exact matches are found
      (bind ?bpmLowerBound (- ?userBeats 5))
      (bind ?bpmUpperBound (+ ?userBeats 5))

      (printout t "No songs found with exactly " ?userBeats " BPM. Recommending songs with similar BPM instead:" crlf)
      
      (do-for-all-facts ((?m Music)) 
         (if (and (<= ?m:beats_per_minute ?bpmUpperBound) 
                  (>= ?m:beats_per_minute ?bpmLowerBound))
            then
            (progn
               (printout t "Title: " ?m:title ", Artist: " ?m:artist ", BPM: " ?m:beats_per_minute crlf)
            )
         )
      )
   )
   (printout t crlf)
)


; case 4, recommend song by year of release
(defrule recommend-by-year
   (UserInput (value 4))
   =>
   (printout t "Enter a production year for a song you want to hear: ")
   (bind ?userYear (read))
   (printout t crlf crlf)

   ; Check if there are any songs from the specified year
   (bind ?songsFromYear (find-all-facts ((?m Music)) (= ?m:year ?userYear)))

   ; If songs from the specified year exist, recommend them
   (if (> (length$ ?songsFromYear) 0)
      then
      (printout t "Recommended songs made in " ?userYear ":" crlf)
      (do-for-fact ((?m ?songsFromYear))
         (printout t "Title: " ?m:title ", Artist: " ?m:artist ", Year: " ?m:year crlf)
      )
      else

      ; Define a range (e.g., +/- 1 year) if no songs are found from the specified year
      (bind ?yrLowerBound (- ?userYear 1))
      (bind ?yrUpperBound (+ ?userYear 1))

      (printout t "No songs found from " ?userYear ". Recommending songs made around " ?userYear ":" crlf)
      
      (do-for-all-facts ((?m Music)) 
         (if (and (<= ?m:year ?yrUpperBound) 
                  (>= ?m:year ?yrLowerBound))
            then
            (progn
               (printout t "Title: " ?m:title ", Artist: " ?m:artist ", Year: " ?m:year crlf)
            )
         )
      )
   )
   (printout t crlf)
)


; case 5, recommend song by popularity 
(defrule recommend-by-popularity
   (UserInput (value 5))
   =>
   (printout t "How popular of a song would you like to discover? Enter a number between 50 (less popular) - 100 (more popular): ")
   (bind ?userPopularity (read))
   (printout t crlf crlf)
   (bind ?songsFound FALSE)
   (printout t "Recommended songs:" crlf)
   (do-for-all-facts ((?m Music)) (eq ?m:popularity ?userPopularity)
      (printout t "Title: " ?m:title ", Artist: " ?m:artist crlf)
      (bind ?songsFound TRUE))
   (if (not ?songsFound)
      then
      (printout t "No songs found for the specified popularity." crlf)
   )
   (printout t crlf)
)

; case 6, recommend song by length in seconds 
(defrule recommend-by-length
   (UserInput (value 6))
   =>
   (printout t "Enter in seconds how long of a song you want recommended: ")
   (bind ?userLength (read))
   (printout t crlf crlf)
   (bind ?songsFound FALSE)
   (printout t "Recommended songs that are " ?userLength " seconds long:" crlf)
   (do-for-all-facts ((?m Music)) (eq ?m:length ?userLength) 
      (printout t "Title: " ?m:title ", Artist: " ?m:artist crlf)
      (bind ?songsFound TRUE))
   (if (not ?songsFound)
      then
      (printout t "No songs found for the specified length." crlf)
   )
   (printout t crlf)
)



; case 7, recommend song by mood (valance)
(defrule recommend-by-mood
   (UserInput (value 7))
   =>
   (printout t "Enter a number between 5-50 for a mellow song or 51-100 for a happy song:  ")
   (bind ?userMood (read))
   (printout t crlf crlf)
   (printout t "Recommended songs within the preferred mood: " crlf)
   
   ; Define a range (e.g., +/- 7 valance)
   (bind ?moodLowerBound (- ?userMood 7))
   (bind ?moodUpperBound (+ ?userMood 7))
   
   (do-for-all-facts ((?m Music)) 
      (if (and (<= ?m:valance ?moodUpperBound) 
               (>= ?m:valance ?moodLowerBound))
         then
         (progn
            (printout t "Title: " ?m:title ", Artist: " ?m:artist crlf)
         )
      )
   )
   
   (printout t crlf)
)

; exit program, when user is done 
(defrule exit-program
   (UserInput (value 8))
   =>
   (printout t "Thank you for using the Music Recommendation System! Now exiting the program." crlf)
   (halt)
)

(defrule invalide-choice
   (UserInput (value ?UserInput&:(or (< ?UserInput 1) (> ?UserInput 8))))
   =>
   (printout t "Invalid Choice! Please restart and enter a number 1-8." crlf)
   (halt)
)